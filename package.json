{
  "name": "schummar-translate",
  "repository": "schummar/schummar-translate",
  "version": "1.9.0",
  "description": "TypeScript powered translation library for React and Node.js.",
  "keywords": [
    "typescript",
    "react",
    "node",
    "translation",
    "i18n",
    "localization",
    "type checking",
    "autocomplete",
    "IntelliSense"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/es/index.mjs"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "import": "./dist/es/react.mjs",
      "require": "./dist/cjs/react.cjs",
      "default": "./dist/es/react.mjs"
    },
    "./astro": {
      "types": "./dist/types/astro/index.d.ts",
      "import": "./dist/es/astro.mjs",
      "require": "./dist/cjs/astro.cjs",
      "default": "./dist/es/astro.mjs"
    },
    "./astroIntegration": {
      "types": "./dist/types/astro/integration.d.ts",
      "import": "./dist/es/astroIntegration.mjs",
      "require": "./dist/cjs/astroIntegration.cjs",
      "default": "./dist/es/astroIntegration.mjs"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "react": [
        "dist/types/react/index.d.ts"
      ],
      "astro": [
        "dist/types/astro/index.d.ts"
      ]
    }
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "MIT",
  "scripts": {
    "build": "runp -i build:clean :p build:compile:* :s build:cjsPackage",
    "build:clean": "rimraf \"dist/**/*\"",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc -p tsconfig.build.json",
    "build:cjsPackage": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "runp -i lint:*",
    "lint:eslint": "eslint src test",
    "lint:tsc": "tsc --noEmit --emitDeclarationOnly false",
    "prepublishOnly": "runp -i lint test build :p publint",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:export": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=test/testResults.xml"
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.6.2",
    "@formatjs/intl-localematcher": "^0.4.2",
    "intl-messageformat": "^10.5.2"
  },
  "devDependencies": {
    "@js-temporal/polyfill": "0.4.4",
    "@schummar/runp": "1.13.2",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitest/coverage-v8": "0.34.4",
    "astro": "3.1.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jsdom": "22.1.0",
    "prettier": "3.0.3",
    "publint": "0.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "semantic-release": "21.1.1",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.4"
  },
  "files": [
    "dist",
    "react.d.ts",
    "react.js"
  ],
  "volta": {
    "node": "18.15.0",
    "pnpm": "7.29.3"
  }
}
